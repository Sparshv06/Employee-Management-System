<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="156.0" prefWidth="170.0" spacing="10" style="-fx-padding: 10;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22" fx:controller="application.AdminController">
    <DatePicker fx:id="startDatePicker" promptText="Start Date" />
    <DatePicker fx:id="endDatePicker" promptText="End Date" />
    <ComboBox fx:id="operatorComboBox" promptText="Select Operator" />
    <Button onAction="#handleOkButtonAction" text="OK">
      <VBox.margin>
         <Insets bottom="10.0" left="55.0" />
      </VBox.margin></Button>
</VBox>
