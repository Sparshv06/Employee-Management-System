<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22" fx:controller="application.TeamLeadController">
    <!-- List all team members -->
    <ComboBox fx:id="teamMembersComboBox" promptText="Select Team Member">
      <VBox.margin>
         <Insets left="30.0" right="30.0" top="40.0" />
      </VBox.margin></ComboBox>
    <Button  onAction="#handleViewTeamMemberButtonAction" text="View Team Member">
      <VBox.margin>
         <Insets bottom="40.0" left="45.0" right="30.0" top="10.0" />
      </VBox.margin></Button>
</VBox>
